{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "api/auth",
      "dest": "routes/auth.js"
    },
    {
      "src": "api/candidates",
      "dest": "routes/candidates.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
