{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "routes/auth.js"
    },
    {
      "src": "/candidates",
      "dest": "routes/candidates.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
